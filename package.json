    "axios": "^1.13.2",
    "drizzle-orm": "^0.44.7",
    "emoji-picker-react": "^4.15.2",
    "expo": "^54.0.0",
    "expo-constants": "~18.0.9",
    "expo-contacts": "~15.0.10",
    "expo-dev-client": "~6.0.18",
    "expo-device": "~8.0.9",
